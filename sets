#removing values from a set
set1.remove(3)
print(set1)
set1.discard(6)
print(set1)
#set operations
a={1,2,3,4,5}
b={4,5,6,7,8}
#union
print(a.union(b))
print(a|b)
#intersection
print(a.intersection(b))
print(a&b)
#difference
print(a-b)
print(b-a)
print(a.difference(b))
#symetric difference
print(a.symmetric_difference(b))
print(a^b)
