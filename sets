set1={1,2,3,4}
print(type(set1))
l1=[5,6,7,8]
#type casting
set2=set([11,12,13])
print(set2)
#adding elements to a set
set1.add(5)
print(set1)
#no indexing, changing values 
#no duplicates
set1.add(4)
print(set1)
"""frozen=frozenset(["a","b","c"])
frozen.add("d")
